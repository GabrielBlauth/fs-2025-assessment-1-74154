# ?? DUBLIN BIKES API - ASSESSMENT 1 - STUDENT 74154
# ?? API Version: V1 (File-based JSON)

### ?? 1. GET ALL STATIONS
GET {{baseUrl}}/api/v1/stations
Accept: application/json

### ?? 2. GET STATION BY NUMBER
GET {{baseUrl}}/api/v1/stations/42
Accept: application/json

### ?? 3. GET STATION BY NUMBER (Non-existent - should return 404)
GET {{baseUrl}}/api/v1/stations/9999
Accept: application/json

### ?? 4. SEARCH STATIONS BY NAME
GET {{baseUrl}}/api/v1/stations/search?q=street
Accept: application/json

### ?? 5. SEARCH STATIONS BY ADDRESS
GET {{baseUrl}}/api/v1/stations/search?q=north
Accept: application/json

### ?? 6. FILTER BY STATUS - OPEN STATIONS
GET {{baseUrl}}/api/v1/stations/search?status=OPEN
Accept: application/json

### ?? 7. FILTER BY STATUS - CLOSED STATIONS
GET {{baseUrl}}/api/v1/stations/search?status=CLOSED
Accept: application/json

### ?? 8. FILTER BY MINIMUM BIKES
GET {{baseUrl}}/api/v1/stations/search?minBikes=10
Accept: application/json

### ?? 9. COMBINED SEARCH - NAME + STATUS + MIN BIKES
GET {{baseUrl}}/api/v1/stations/search?q=square&status=OPEN&minBikes=5
Accept: application/json

### ?? 10. GET STATION SUMMARY
GET {{baseUrl}}/api/v1/stations/summary
Accept: application/json

